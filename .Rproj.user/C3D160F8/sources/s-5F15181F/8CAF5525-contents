\name{verify.key}

\alias{verify.key}
\alias{verify.key.default}
\alias{verify.key.data.frame}

\title{
   Check an Index Key
}

\description{
   Checks whether a data frame's index key is unique or if it contains any
   missing values. An index key is a combination of variables which uniquely
   identify data rows in a table. Index keys are important for merging data
   tables.
}

\usage{
   # Generic 'verify.key' method:
   verify.key(x, ...)

   # Default 'verify.key' method:
   verify.key.default(x, ...)

   # 'data.frame' 'verify.key' method:
   verify.key.data.frame(x, key, ...)
}

\arguments{
   \item{x}{An \sQuote{data.frame} or \sQuote{ascii} object.}

   \item{key}{A character string specifying the variables which define the index key.}
   
   \item{\ldots}{Further arguments to be passed onto the 
                 \code{\link[gulf]{msg.str}} function.}
}

\details{

}

\value{
   A character vector of error messages. Returns \code{NULL} if no errors are
   detected.
}

\author{
   Tobie Surette \email{Tobie.Surette@dfo-mpo.gc.ca} \cr
   Pablo Vergara \email{Pablo.Vergara@dfo-mpo.gc.ca}
}

\seealso{
   \code{\link[gulf]{verify}},
   \code{\link[gulf]{key}},
   \code{\link[gulf]{msg.str}}
}

\examples{

}
